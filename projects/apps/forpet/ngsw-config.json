{
  "$schema": "../../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "cacheGetRequests",
      "urls": [
        "/api/GoodGroups/GetGoodGroups",
        "/api/Goods/GetBrands?page=1&per_page=-1"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1h",
        "strategy": "performance"
      }
    }
  ]
}